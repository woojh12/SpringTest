<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.dnjh.spring.test.ajax.repository.FavoriteRepository">
 	<select id="selectFavorite" resultType="com.dnjh.spring.test.ajax.domain.Favorite">
 		SELECT
 			`id`
 			, `name`
 			, `url`
 		FROM
 			`favorite`
	</select>
</mapper>